name="SQL 1">SELECT
  id,
  game_date,
  season,
  home_team,
  away_team,
  home_score,
  away_score,
  home_is_winner,
  away_is_winner,
  game_type,
  commence_time,
  (
    SELECT COUNT(*)
    FROM past_games_update2 AS inner_tbl
    WHERE ((inner_tbl.home_team = outer_tbl.home_team AND inner_tbl.home_is_winner = 1)
      OR (inner_tbl.away_team = outer_tbl.home_team AND inner_tbl.away_is_winner = 1))
      AND inner_tbl.game_date &lt; outer_tbl.game_date
      AND inner_tbl.season = outer_tbl.season
      AND inner_tbl.game_type = 'R'
  ) AS home_team_win_count,
  (
    SELECT COUNT(*)
    FROM past_games_update2 AS inner_tbl
    WHERE ((inner_tbl.home_team = outer_tbl.away_team AND inner_tbl.home_is_winner = 1)
      OR (inner_tbl.away_team = outer_tbl.away_team AND inner_tbl.away_is_winner = 1))
      AND inner_tbl.game_date &lt; outer_tbl.game_date
      AND inner_tbl.season = outer_tbl.season
      AND inner_tbl.game_type = 'R'
  ) AS away_team_win_count
FROM past_games_update2 AS outer_tbl
WHERE outer_tbl.game_type = 'R'
ORDER BY game_date ASC;</sql><current_tab id="0"/></tab_sql></sqlb_project>
